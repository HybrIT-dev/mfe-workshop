{
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "name": "flight-page-flight-home",
  "sourceRoot": "libs/flight/pages/page-flight-home/_index",
  "projectType": "library",
  "prefix": "page",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/flight/pages/page-flight-home/_index"],
      "options": {
        "project": "libs/flight/pages/page-flight-home/_index/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/flight/pages/page-flight-home/_index/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/flight/pages/page-flight-home/_index/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/flight/pages/page-flight-home/_index/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/flight/pages/page-flight-home/_index/**/*.ts",
          "libs/flight/pages/page-flight-home/_index/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:page", "domain:flight", "page:flight-home"]
}
