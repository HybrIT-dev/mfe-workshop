{
  "name": "flight-features-flight-search",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/flight/features/flight-search/src",
  "prefix": "feature",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/flight/features/flight-search"],
      "options": {
        "project": "libs/flight/features/flight-search/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/flight/features/flight-search/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/flight/features/flight-search/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/flight/features/flight-search"],
      "options": {
        "jestConfig": "libs/flight/features/flight-search/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/flight/features/flight-search/**/*.ts",
          "libs/flight/features/flight-search/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:flight", "type:feature"]
}
