{
  "name": "passenger-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/passenger/api/src",
  "prefix": "flight-workspace",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/passenger/api"],
      "options": {
        "project": "libs/passenger/api/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/passenger/api/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/passenger/api/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/passenger/api"],
      "options": {
        "jestConfig": "libs/passenger/api/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/passenger/api/**/*.ts",
          "libs/passenger/api/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:passenger", "domain:passenger-api", "type:api"]
}
