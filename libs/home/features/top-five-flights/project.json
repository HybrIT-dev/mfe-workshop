{
  "name": "home-features-top-five-flights",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/home/features/top-five-flights/src",
  "prefix": "feature",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/home/features/top-five-flights"],
      "options": {
        "project": "libs/home/features/top-five-flights/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/home/features/top-five-flights/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/home/features/top-five-flights/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/home/features/top-five-flights"],
      "options": {
        "jestConfig": "libs/home/features/top-five-flights/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/home/features/top-five-flights/**/*.ts",
          "libs/home/features/top-five-flights/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:home", "type:feature"]
}
